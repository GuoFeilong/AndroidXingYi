        <attr name="indicator_color" format="color" />
    </declare-styleable>

    <declare-styleable name="ATCircleView">
        <attr name="circle_width" format="dimension" />
        <attr name="circle_corner" format="dimension" />
        <attr name="view_bg_color" format="color" />
    </declare-styleable>

    <declare-styleable name="ATDotPointView">
        <attr name="point_view_selector_background" format="reference" />
        <attr name="point_view_point_size" format="dimension" />
        <attr name="point_view_point_margin" format="dimension" />
    </declare-styleable>

    <declare-styleable name="RoundProgressbar">
        <attr name="version_update_background_color" format="color" />
        <attr name="version_update_progress_color" format="color" />
    </declare-styleable>


</resources>